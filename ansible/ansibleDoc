// Este ejemplo nos ense√±a como instalar el index.html utilizando templates

- name: Install index.html
      template:
        src: templates/index.html
        dest: /var/www/hmtl.index.html
        owner: www-data
        grouo: www-data
        mode: '0644'


generate_ssh_key: no
        ssh_key_bits: 2048
        ssh_key_file: .ssh/app